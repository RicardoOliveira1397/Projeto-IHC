{
    "fileWatcherPlugin": {
        "touchFile": "./pages/_document.js",
        "ignored": "./pages/_document.js",
        "watchFileGlobs": [
            "./pages/**/*.js",
            "./pages/**/*.css",
            "./pages/**/*.php"
        ]
    },
    "wpThemeServer": {
        "enable": true,
        "host": "127.0.0.1",
        "port": 8090,
        "sslCert": null,
        "sslKey": null,
        "watchFile": "../index.php"
    },
    "injectWpThemeClient": {
        "override": null,
        "file": "./next/static/runtime/main.js"
    }
}